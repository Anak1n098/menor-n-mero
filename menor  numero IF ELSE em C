#include <stdio.h>

int main() {
    
    int num1, num2, num3;
    
//entrada de dados
    printf("\n\tDigite 3 valores inteiros distintos: ");
    scanf("%d%d%d", &num1, &num2, &num3);
    
//processamento de dados
    if ((num1 < num2) && (num1 < num3))
        printf("\n\t%d é o menor número", num1);
        
    else
        if (num2 < num3)
            printf("\n\t%d é o menor número", num2);
    
    return 0;
}
